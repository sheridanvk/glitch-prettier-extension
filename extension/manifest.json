{
  "manifest_version": 2,
  "name": "Prettier Glitch",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_security_policy": "script-src 'self' https://glitch-prettier-extension.glitch.me; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": ["page-scraper.css"],
      "js": [
        "jquery-3.2.1.min.js", 
        "standalone.js",
        "parser-babylon.js",
        "parser-graphql.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "frog_guiro_128.jpg"
  },
  "web_accessible_resources": [
    "script.js"
  ]
}
